<!-- wetoast.wxml -->
<template name="tk-alert-tip">
  <!-- 自定义弹窗 -->
  <view class="showModule fade_in" wx:if="{{text}}" >
    <view class="showModuleContent">
        <view class="text ">{{text}}</view>
    </view>
  </view>
</template>


<template name="tk-flytop-tip">
 <view class="notice_module flytop" wx:if="{{msg}}">
    <view class="shownotice_module">
        <view class="text ">{{msg}}</view>
    </view>
  </view>
  </template>