.wraper .listItems{
    padding: 0 20px;
    height: 80rpx;
    margin-top: 10px;
    box-sizing: border-box;
text-align: center;
    border-bottom: 1rpx solid #f3f3f3;
}
.wraper .listItems .listLeft{
    float: left;
}
.wraper .listItems .listRight{
    float: left;
    margin-left: 20rpx;
    width: 420rpx;
}
.btnGroup .btn{
    width: 80%;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 45rpx;
    background: #f12035;
    color: #fff;
    margin-top: 30rpx;
}